{
	"name": "vsc/orm",
	"type": "library",
	"description": "A small collection of PHP classes for abstracting database access",
	"repositories": [
	{
		"type": "vcs",
		"url": "https://github.com/lpbm/vsc.git"
	}
	],
	"require": {
	"php": ">=5.5.0",
	"vsc/vsc": "dev-master"
	},
	"minimum-stability": "stable",
	"license": "MIT",
	"authors": [
	{
		"name": "Marius Orcsik",
		"email": "marius@habarnam.ro",
		"role": "developer"
	}
	],
	"autoload": {
	"psr-4" : { "orm\\" : ["lib/", "res/"]}
	},
	"autoload-dev": {
	"psr-4" : { "_fixtures\\" : "tests/_fixtures"}
	}
}
