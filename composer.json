{
	"name": "vsc/orm",
	"type": "library",
	"description": "A small collection of PHP classes for abstracting database access",
	"repositories": [
	{
		"type": "vcs",
		"url": "https://github.com/lpbm/vsc.git"
	}
	],
	"require": {
		"php": "~5.6 || ~7.2",
		"vsc/vsc": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "~5.7",
		"doctrine/instantiator": "<1.1.0"
	},
	"minimum-stability": "stable",
	"license": "MIT",
	"authors": [
	{
		"name": "Marius Orcsik",
		"email": "marius@habarnam.ro",
		"role": "developer"
	}
	],
	"autoload": {
		"psr-4" : { "orm\\" : ["src/"]}
	},
	"autoload-dev": {
		"psr-4" : { "mocks\\" : "tests/mocks"}
	}
}
